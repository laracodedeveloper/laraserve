{
    "name": "laracodedeveloper/laraserve",
    "description": "A more enjoyable local development experience for Mac.",
    "keywords": ["laravel", "laracodes", "webdeveloper", "laracodedeveloper"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sami Maxhuni",
            "email": "samimaxhuni510@gmail.com"
        }
    ],
    "autoload": {
        "files": [
            "cli/includes/compatibility.php",
            "cli/includes/facades.php",
            "cli/includes/helpers.php"
        ],
        "psr-4": {
            "Laraserve\\": "cli/Laraserve/"
        }
    },
    "require": {
        "php": ">=5.6",
        "illuminate/container": "~5.1|^6.0|^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "mnapoli/silly": "~1.0",
        "symfony/process": "~3.0|~4.0|~5.0",
        "nategood/httpful": "~0.2",
        "tightenco/collect": "^5.3|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.2.3",
        "phpunit/phpunit": "~5.7"
    },
    "bin": [
        "laraserve"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
